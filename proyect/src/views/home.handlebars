<div class="products-container">
  {{#each products}}
    <div class="product-card">
      <img src="{{this.thumbnail}}" alt="{{this.title}} Thumbnail" />
      <h2>{{this.title}}</h2>
      <p>Precio: ${{this.price}}</p>
      <p>Stock : {{this.stock}}</p>
      <p>description: {{this.description}}</p>
      <p>Category: {{this.category}}</p>
    </div>
  {{/each}}
</div>
